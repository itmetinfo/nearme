<% if object.state.present? %> 
<%= select_tag 'product[state]', options_for_select(State.all.map { |k| [k.name, k.id] }, :selected => State.find(object.state), :include_blank => 'None'), { data: { enumeration: true} } %>
<% else %>
<%= select_tag 'product[state]', options_for_select(State.all.map { |k| [k.name, k.id] }, :include_blank => 'None'), { data: { enumeration: true} } %>
<% end %>